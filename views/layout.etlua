<!doctype html>


<!--
  site generated by lapis web framework
  configuration: <%= configuration._name %>
  port: <%= configuration.port %>
  ssl_port: <%= configuration.ssl_port %>
-->

<html>
  <link rel="stylesheet" href="/static/style.css" type="text/css" />
  <!-- <link rel="icon" href="/static/dlkey.svg" type="image/svg+xml" /> -->
  <link rel="icon" href="/static/dlkey_icon.svg.png" type="image/png" />

  <body>
    <header>
      <!-- Change this URL if you want, I won't complain if you don't though xD -->
      <a href="//www.darkwiiplayer.com" target="_blank">
        <h1><img src="/static/dlkey.svg" style="height: 1em; margin-right: 0.4em;" /> dl_Keys</h1>
      </a>
      <nav>
        <% for i,page in pairs(nav) do %>
          <a href="<%=url_for(page:lower():gsub(" ","_"))%>"><%=page%></a> <!-- These links are all dynamically generated -->
        <% end %>
        <a href="//google.com" target="_blank">google.com</a> <!-- Just as an example for a static linc -->
      </nav>
    </header>

    <main>
      <% content_for "inner" %>
    </main>
  </body>
  
  <%
    if not no_footer then render "views.footer" end
  %>
</html>